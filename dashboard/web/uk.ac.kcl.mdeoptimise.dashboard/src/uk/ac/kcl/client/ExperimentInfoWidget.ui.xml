<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	.text {
		color: #595959;
	}
	.container {
		padding-left: 60px;
		padding-right: 60px;
		
	}
	div {
		padding-bottom: 5px;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.container}">
		<div >
			<b:Label type="INFO">Experiment info:</b:Label>
			<g:Label styleName="{style.text}" ui:field="experimentId" >ID: </g:Label>
			<g:Label styleName="{style.text}" ui:field="workerId" >Worker MAC Address: </g:Label>
			<g:Label styleName="{style.text}" ui:field="startTime" >Start Time: </g:Label>
			<g:Label styleName="{style.text}" ui:field="endTime" >End Time: </g:Label>
			<g:Label styleName="{style.text}" ui:field="totalTime" >Total Time Taken: </g:Label>
		</div>
		<div >
			<b:Label type="INFO">MOPT info:</b:Label>
			<g:Label styleName="{style.text}" ui:field="moptId" >ID: </g:Label>
			<g:Label styleName="{style.text}" ui:field="model" >Model: TTC_InputRDG_C.xmi</g:Label>
			<g:Label styleName="{style.text}" ui:field="metamodel" >Metamodel: architectureCRA.ecore</g:Label>
		</div>
		<g:SimplePanel ui:field="solutionsTable" />
	</g:HTMLPanel>
</ui:UiBinder> 