      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.compare"/>
      <import plugin="org.eclipse.xtext.builder"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.jdt.launching" version="3.8.100" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.debug.ui" version="3.7.201" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="uk.ac.kcl.mdeoptimise"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="uk.ac.kcl.mdeoptimise.ide"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="uk.ac.kcl.mdeoptimise.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

